{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": ["warn", "always"],
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-unused-vars": [
      "warn", { 
        "vars": "all" 
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "plugin:react/recommended",
    "prettier"
  ]
}